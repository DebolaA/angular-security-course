{
  "name": "angular-security-course",
  "version": "0.0.0",
  "license": "MIT",
  "notes": {
    "extra-cli-options": "--ssl --ssl-key key.pem --ssl-cert cert.pem"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config ./proxy.json --ssl --ssl-key key.pem --ssl-cert cert.pem",
    "start-server": "ts-node ./server/server.ts --secure",
    "server": "nodemon  -w ./server --ext \".ts\" --exec \"npm run start-server\"",
    "hash": "node ./demos/hash.js",
    "hash-salt": "node ./demos/hash-salt.js",
    "verify-password": "node ./demos/verify-password.js",
    "jwt-hs256": "node ./demos/jwt-hs256.js",
    "jwt-check-hs256": "node ./demos/jwt-check-hs256.js",
    "jwt-rs256": "node ./demos/jwt-rs256.js",
    "jwt-check-rs256": "node ./demos/jwt-check-rs256.js",
    "csrf-server": "./node_modules/.bin/http-server -c-1 .",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.7",
    "@angular/common": "^17.3.7",
    "@angular/compiler": "7.0.0",
    "@angular/core": "^10.2.5",
    "@angular/forms": "^17.3.7",
    "@angular/platform-browser": "^17.3.7",
    "@angular/platform-browser-dynamic": "^17.3.7",
    "@angular/router": "^17.3.7",
    "@types/express": "^4.0.36",
    "@types/lodash": "^4.14.70",
    "argon2": "^0.28.2",
    "body-parser": "^1.17.2",
    "command-line-args": "^4.0.6",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "http-server": "^14.1.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.18.1",
    "nodemon": "^3.1.0",
    "password-validator": "^4.0.0",
    "update": "^0.4.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.6",
    "@angular/cli": "^10.2.1",
    "@angular/compiler-cli": "^10.2.5",
    "@angular/language-service": "7.0.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^0.0.28",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "^6.4.3",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.4",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "3.1.3"
  }
}
